{
  "name": "azdo-pr-agent-extension",
  "version": "3.3.0",
  "description": "AI-powered Pull Request agent for Azure DevOps - Automated code review, improvement suggestions, and compliance checks",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "tfx extension create --manifest-globs vss-extension.json",
    "package:dev": "tfx extension create --manifest-globs vss-extension-dev.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "keywords": [
    "azure-devops",
    "extension",
    "pull-request",
    "ai",
    "code-review",
    "automation"
  ],
  "author": "Mohammad Al Rousan",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "jest": "^29.6.0",
    "rimraf": "^5.0.0",
    "sass": "^1.64.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "tfx-cli": "^0.21.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "azure-devops-extension-api": "^4.231.0",
    "azure-devops-extension-sdk": "^4.0.2",
    "azure-devops-ui": "^2.167.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
